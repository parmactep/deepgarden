resetHeader()
	display: inline-block
	padding: 0
	margin: 0
	font-size: inherit
	font-weight: inherit

resetLink()
	text-decoration: none
	color: inherit	

resetList()
	margin: 0
	padding: 0
	list-style: none

resetInput()
	font-family: inherit
	border: none
	background: transparent
	color: inherit
	cursor: inherit
	height: 34px
	padding: 0

	&:focus
		outline: none